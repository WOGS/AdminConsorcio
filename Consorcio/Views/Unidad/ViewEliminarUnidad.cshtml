@model ServicioNegocio.EF.Unidad

@{
    Layout = "~/Views/Shared/_LayoutGeneral.cshtml";
}
<body>
    <h1>¿Esta seguro que desea eliminar la Unidad?</h1>
    <div class="form-group">
        <div>@TempData["idUnidad"]</div>
        <button type="submit" class="btn btn-success btn-lg">
            <a class="underlineHover" type="" href="/Unidad/Eliminar/@TempData["idUnidad"]"> Confirmar</a>
        </button>

        <button type="button" class="btn btn-success btn-lg">
            <a class="underlineHover" type="" href="/Unidad/Listar">Cancelar</a>
        </button>
    </div>
</body>

