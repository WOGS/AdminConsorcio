@model ServicioNegocio.EF.Gasto
@{
        Layout = "~/Views/Shared/_LayoutEliminar.cshtml";
}
<section id="listado" class="ta-c">

    <h1>¿Esta seguro que desea eliminar el Gasto?</h1>
    <div class="form-group">
        <div hidden>@TempData["idGasto"]</div>
        <a class="underlineHover btn-primary btn-lg" type="button" href="/Gasto/Eliminar/@TempData["idGasto"]"> Confirmar</a>
        <a class="underlineHover btn-primary btn-lg" type="button"  href="/Gasto/Listar">Cancelar</a>
    </div>

</section>