
@{ Layout = "~/Views/Shared/_LayoutEliminar.cshtml"; }

<section id="listado" class="ta-c">
    <body>
        <h1>¿Esta seguro que desea eliminar el consorcio?</h1>
        <div  class="form-group">
            <div hidden>@TempData["idConsorcio"]</div>
            <button type="submit" class="btn btn-success btn-lg">
                <a class="underlineHover" type="" href="/Consorcio/Eliminar/@TempData["idConsorcio"]"> Confirmar</a>
            </button>
            <button type="button" class="btn btn-success btn-lg">
                <a class="underlineHover" type="" href="/Consorcio/Listar">Cancelar</a>
            </button>
        </div>
    </body>
    </section>